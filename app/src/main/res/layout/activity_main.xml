<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.news.channy.retrofitvsvolley.MainActivity">


    <Button
        android:id="@+id/buttonRetrofit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="27dp"
        android:text="Retrofit" />

    <Button
        android:id="@+id/buttonVolley"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/buttonRetrofit"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:text="Volley" />

    <AutoCompleteTextView
        android:id="@+id/textViewResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/buttonVolley"
        android:layout_alignLeft="@+id/buttonRetrofit"
        android:layout_alignRight="@+id/buttonVolley"
        android:layout_alignStart="@+id/buttonRetrofit"
        android:layout_below="@+id/buttonRetrofit"
        android:layout_marginTop="80dp"
        android:text="Result" />
</RelativeLayout>
